<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.androidtown.hello">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewActivity"></activity>
        <activity android:name=".NewActivity2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".NewActivity2_removeActivity">
            <intent-filter>
                <action android:name="NewActivity2_AlarmRemove">
                </action>
            </intent-filter>
        </activity>
        <receiver android:name=".NewActivity2_AlarmReceiver" android:exported="true">
            <intent-filter>
                <action android:name="NewActivity2_AlarmReceiver"></action>
            </intent-filter>
        </receiver>
        <activity android:name=".NewActivity3"></activity>
        <activity android:name=".NewActivity4"></activity>
        <activity android:name=".NewActivity5"></activity>
        <activity android:name=".NewActivity1_3"></activity>
        <activity android:name=".NewActivity3_1"
            android:label="@string/act_med_form">
        </activity>
    </application>

</manifest>