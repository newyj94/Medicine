<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.androidtown.hello">
    <uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    //지도퍼미션
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"></uses-permission>>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_OWNER_DATA"></uses-permission>
    <uses-permission android:name="android.permission.READ_OWNER_DATA"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    //알람
    <uses-permission android:name="android.permisson.WAKE_LOCK"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewActivity"></activity>
        <activity android:name=".NewActivity2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".NewActivity2_removeActivity">
            <intent-filter>
                <action android:name="NewActivity2_removeActivity">
                </action>
            </intent-filter>
        </activity>
        <receiver android:name=".NewActivity2_AlarmReceiver" android:exported="true">
            <intent-filter>
                <action android:name="NewActivity2_AlarmReceiver"></action>
            </intent-filter>
        </receiver>
        <activity android:name=".NewActivity3"></activity>
        <activity android:name=".NewActivity4"></activity>
        <activity android:name=".NewActivity5"></activity>
        <activity android:name=".NewActivity1_1"></activity>
        <activity android:name=".NewActivity1_2"></activity>
        <activity android:name=".NewActivity1_3" android:windowSoftInputMode="stateHidden"></activity>
        <activity android:name=".NewActivity1_4"></activity>
        <activity android:name=".NewActivity3_1"
            android:label="@string/act_med_form">
        </activity>
    </application>

</manifest>